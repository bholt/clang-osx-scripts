#!/bin/bash
DIR=$( cd $(dirname $0) ; pwd -P )
exec $DIR/clang-3.4 -stdlib=libc++ -nostdinc++ -isystem /Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/../lib/c++/v1 -isystem /Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib -I/usr/local/include -lc++ -Qunused-arguments "$@"